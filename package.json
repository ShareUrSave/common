      "optional": false
    },
    "prisma": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2"
