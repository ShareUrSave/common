{
  "name": "@share-ur-save/common",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "common": "./bin/common-cli.js"
  },
